{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "variable-name": [true, "allow-leading-underscore"],
    "no-console": false,
    "no-submodule-imports": [true, "module-alias", "@components", "@config", "@services"],
    "no-implicit-dependencies": [true, "dev", ["@components", "@config", "@services"]]
  }
}
