{
	"name": "aionic-core",
	"version": "0.0.1",
	"description": "The core API required for all other Aionic applications",
	"main": "./dist/index.js",
	"author": "Lars WÃ¤chter <lars97.waechter@gmail.com>",
	"private": true,
	"homepage": "https://aionic.org",
